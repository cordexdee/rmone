

@{
    ViewBag.Title = "UserAdminRepository";
    Layout = "~/Views/Shared/_DMSLayout.cshtml";
    var isAdmin = ViewBag.role;
}

<div >
    @using (Html.BeginForm("", "", new { @id = "UserAdminRepositoryForm" }))
    {
        <div class="row">
            <div class="col-md-8">
                <div class="row permission-search-box">
                    <div class="col-md-7">
                        @Html.Editor("UserName", new { htmlAttributes = new { @class = "form-control fa fa-search search-icon", @id = "UserName", @required = "required", @placeholder = "Search By User Name" } })
                    </div>
                    <div class="col-md-2">
                        <input type="button" id="SearchUserPermissionId" value="Submit" class="btn btn-primary" />
                    </div>
                    @Html.Hidden("UserId", new { htmlAttributes = new { @class = "form-control", @id = "UserId" } })
                    
                </div>
            </div>
        </div>
    }
    <div id="msgDiv" class="col-md-offset-2 col-md-10 text-warning"></div>
    <div id="successDiv" class="col-md-offset-2 col-md-10 text-success log"></div>
    <div id="errorDiv" class="col-md-offset-2 col-md-10 text-danger errorLog"></div>
</div>
<div id="loading-image" class="modal" data-keyboard="false">
    
</div>
<div id="fileDirectoryList">
</div>
<div style="width: 100%; margin-top: 24px;">
    <input type="button" id="SavePermissionDetailId" value="Save-Details" class="savedetails" style="display:none;" />
</div>





