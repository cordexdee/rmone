

@{
    Layout = "~/Views/Shared/AdminLayouts.cshtml";
    ViewBag.Title = "Dashboard";
}




<div style="width:60%;display: block;margin: auto;">
    <p class="heading">Dashboards</p>
    <div style="width: 100%;float: left;">
        <div class="first-block">
            <div class="inner center-bullet" style="width: 150px;height:87px;float: left;padding: 40px 0px 0px;margin-top: 0px;float: left; border-right:2px solid rgb(222, 181, 153);">
                <img class="icon_size" src="~/Content/Images/Admin/Configure/Dashboards/book-opened.png" />
                <p class="icon_text" style="padding-top:5px;">Select Workflow</p>
            </div>
            <div id="mixedSlider" data-wrap="false">
                <div class="MS-content">

                    <div class="item">
                        <img class="icon_size bar" src="~/Content/Images/Admin/Configure/Dashboards/save-technique.png" style="cursor: pointer;" onclick="return Admin.loadModule('Dashboard', '/Layouts/uGovernIT/uGovernITConfiguration.aspx?control=configdashboard', 'Dashboards & Queries', '90', '90', '', '');" />
                        <div class="line"><span></span></div>
                        <p class="icon_text svgicon">Incident (TSR)</p>
                    </div>
                    <div class="item">
                        <img class="icon_size bar" src="~/Content/Images/Admin/Configure/Dashboards/patent-application.png" onclick="return Admin.loadModule('Dashboard','/Layouts/uGovernIT/uGovernITConfiguration.aspx?control=configdashboard','Dashboards & Queries','90','90','','')" />
                        <div class="line"><span></span></div>
                        <p class="icon_text text-center">Change Request (ACR)</p>
                    </div>
                    <div class="item">
                        <img class="icon_size bar" src="~/Content/Images/Admin/Configure/Dashboards/secure-server.png" onclick="return Admin.loadModule('Dashboard','/Layouts/uGovernIT/uGovernITConfiguration.aspx?control=configdashboard','Dashboards & Queries','90','90','','')" />
                        <div class="line"><span></span></div>
                        <p class="icon_text text-center">Change Control (DRQ)</p>
                    </div>
                    <div class="item">
                        <img class="icon_size bar" src="~/Content/Images/Admin/Configure/Dashboards/cause-trouble.png" onclick="return Admin.loadModule('Dashboard','/Layouts/uGovernIT/uGovernITConfiguration.aspx?control=configdashboard','Dashboards & Queries','90','90','','')" />
                        <div class="line"><span></span></div>
                        <p class="icon_text">Problem Analysis (RCA)</p>
                    </div>
                    <div class="item">
                        <img class="icon_size bar" src="~/Content/Images/Admin/Configure/Dashboards/give-check-mark.png" onclick="return Admin.loadModule('Dashboard','/Layouts/uGovernIT/uGovernITConfiguration.aspx?control=configdashboard','Dashboards & Queries','90','90','','')" />
                        <div class="line"><span></span></div>
                        <p class="icon_text text-center">Task List (TSK)</p>
                    </div>
                </div>
                <div class="MS-controls">
                    <button class="MS-left"><i class="fa fa-angle-left fa-2x" aria-hidden="true"></i></button>
                    <button class="MS-right"><i class="fa fa-angle-right fa-2x" aria-hidden="true"></i></button>
                </div>
            </div>
        </div>
    </div>
    <div style="width: 100%;float: left;">
        <div class="second-block">
            <div class="inner center-bullet" style="width: 150px;height:87px;padding: 40px 0px 0px;margin-top: 0px;float: left; border-right:2px solid rgb(222, 181, 153);">
                <img class="icon_size" src="~/Content/Images/Admin/Configure/Dashboards/to-do-list.png" />
                <p class="icon_text" style="padding-top:5px;">List Existing Dashboard..</p>
            </div>
            <div id="mixedSlider2">
                <div class="MS-content">

                    <div class="item">
                        <img class="icon_size bar" src="~/Content/Images/Admin/Configure/Dashboards/copywriting.png" onclick="return Admin.loadModule('Dashboard','/Layouts/uGovernIT/uGovernITConfiguration.aspx?control=configdashboard','Dashboards & Queries','90','90','','')" />
                        <div class="line"><span></span></div>
                        <p class="icon_text">Edit</p>
                    </div>

                    <div class="item">
                        <img class="icon_size bar" src="~/Content/Images/Admin/Configure/Workflows/edit-a-document.png" onclick="return Admin.loadModule('Dashboard','/Layouts/uGovernIT/uGovernITConfiguration.aspx?control=configdashboard','Dashboards & Queries','90','90','','')" />
                        <div class="line"><span></span></div>
                        <p class="icon_text">New</p>
                    </div>

                    <div class="item">
                        <img class="icon_size bar" src="~/Content/Images/Admin/Configure/Dashboards/newsstand.png" onclick="return Admin.redirectModule('../Pages/TSKProjects');" />
                        <div class="line"><span></span></div>
                        <p class="icon_text text-center">Advanced Dashboard Designer</p>
                    </div>

                </div>


            </div>
        </div>
    </div>
    <div style="width: 100%;float: left;">
        <div class="third-block">
            <div class="inner center-bullet" style="width: 150px;height:87px;padding: 40px 0px 0px;margin-top: 0px;float: left; border-right:2px solid rgb(222, 181, 153);">
                <img class="icon_size" src="~/Content/Images/Admin/Configure/Dashboards/company.png" />
                <p class="icon_text" style="padding-top:5px;">Organize Dashboard</p>
            </div>
            <div id="mixedSlider3">
                <div class="MS-content">

                    <div class="item">
                        <img class="icon_size bar" src="~/Content/Images/Admin/Configure/Dashboards/gauge.png" onclick="return Admin.loadModule('Dashboard','/Layouts/uGovernIT/uGovernITConfiguration.aspx?control=configdashboardviews','Dashboards & Views','600px;','500px;','','');" />
                        <div class="line"><span></span></div>
                        <p class="icon_text">Preview Dashboard</p>
                    </div>
                    <div class="item">
                        <img class="icon_size bar" src="~/Content/Images/Admin/Configure/Dashboards/translation-icon.png" />
                        <div class="line"><span></span></div>
                        <p class="icon_text">Update Dashboard Group</p>
                    </div>

                </div>

            </div>
        </div>
    </div>
</div>

